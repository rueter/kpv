      <mg relId="2">
     <tg xml:lang="eng">
       <t pos="N">jibberish</t>
     </tg>
     <tg xml:lang="fin">
       <t pos="N">(ei) mikään</t>
       <t pos="N">(ei) mitään</t>
       <t pos="N">hölynpöly</t>
     </tg>
         <tg xml:lang="rus">
	   <t>(в роли существительного)</t>
            <t pos="N">пустое, вздор</t>
         </tg>
         <xg>
            <x>нинӧмсӧ эн сӧр</x>
            <xt xml:lang="rus">не мели вздор</xt>
         </xg>
         <xg>
            <x>нинӧм абу</x>
            <xt xml:lang="fin">ei ole mitään</xt>
            <xt xml:lang="rus">ничего нет</xt>
            <xt xml:lang="rus">пустяки, пустое</xt>
            <xt xml:lang="rus">вздор, чушь, чепуха</xt>
            <xt xml:lang="rus">глупости</xt>
         </xg>
         <xg>
            <x>нинӧм абусӧ висьтавны</x>
            <xt xml:lang="fin">puhua joutavia</xt>
            <xt xml:lang="fin">puhuä pötyä</xt>
         </xg>
         <xg>
            <x>нинӧм абусӧ висьтавны</x>
            <xt xml:lang="rus">говорить глупости, говорить чепуху</xt>
         </xg>
         <xg>
            <x>нинӧм абу сӧрны</x>
            <xt xml:lang="rus">молоть ерунду, молоть вздор</xt>
            <xt xml:lang="rus">нести чушь</xt>
         </xg>
         <xg>
            <x>нинӧм изны</x>
            <xt xml:lang="rus">молоть ерунду, молоть вздор</xt>
            <xt xml:lang="rus">нести чушь</xt>
         </xg>
         <xg>
            <x>нинӧм абуысь</x>
            <xt xml:lang="rus">за пустяки, по пустякам, беспричинно, из-за пустяков</xt>
         </xg>
         <xg>
            <x>нинӧм абусьыс оз сулав лёксьыны</x>
            <xt xml:lang="rus">из-за пустяков не стоит ссориться</xt>
         </xg>
         <xg>
            <x>нинӧм абуысь гез гартны</x>
            <xt xml:lang="rus">делать из мухи слона</xt>
            <xt t_type="literally:буквально" xml:lang="rus">из ничего вить верёвку</xt>
         </xg>
         <xg>
            <x>нинӧм абу туйын овны</x>
       <xt xml:lang="rus">держать себя, вести себя скромно, держаться в тени (не выказывая своих знаний, уменья)</xt>
         </xg>
         <xg>
            <x>нинӧм дон оз сулав</x>
            <xt xml:lang="fin">ei ole minkään arvoinen</xt>
            <xt xml:lang="fin">olla arvoton</xt>
         </xg>
         <xg>
            <x>нинӧм дон оз сулав</x>
            <xt xml:lang="rus">ничего не стоит</xt>
         </xg>
         <xg>
            <x>меным тайӧ нинӧм дон оз сулав</x>
            <xt xml:lang="fin">Tämä ei ole minulle minkään arvoinen.</xt>
            <xt xml:lang="fin">Tämä on minulle arvoton.</xt>
            <xt xml:lang="rus">мне это ничего не стоит</xt>
            <xt xml:lang="rus">мне это нипочём</xt>
         </xg>
         <xg>
            <x>пӧръявны сылы нинӧм (дон) оз сулав</x>
            <xt xml:lang="rus">ему ничего не стоит соврать</xt>
         </xg>
         <xg>
            <x>сійӧ ачыс нинӧм дон оз сулав</x>
            <xt xml:lang="fin">Hän itse on vähäpätöinen.</xt>
            <xt xml:lang="fin">Hän itse on merkityksetön.</xt>
            <xt xml:lang="fin">Hän itse on mitätön.</xt>
            <xt xml:lang="rus">сам он ничего не стоит, сам он никчёмный</xt>
            <xt xml:lang="rus">сам он никудышный</xt>
         </xg>
     <xg>
       <x><wf msd="Ill">нинӧмӧ</wf> воны</x>
       <xt t_type="word-for-word:буквально" xml:lang="rus">превратиться в ничто, стать ничем; потерять здоровье, превратиться в калеку</xt>
     </xg>
     <xg>
       <x><wf msd="Ill">нинӧмӧ</wf> пӧрны</x>
       <xt t_type="word-for-word:буквально" xml:lang="rus">превратиться в ничто, стать ничем; потерять здоровье, превратиться в калеку</xt>
     </xg>
